<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jroldan.foreignintruders">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_earthovnis_background"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_earthovnis_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.ForeignIntrudersTheme">
        <activity android:name=".ui.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.main.views.ForeignIntrudersActivity"/>
        <activity android:name=".ui.login.LoginViewImpl"/>
        <activity android:name=".ui.about.AboutActivity"/>
    </application>

</manifest>